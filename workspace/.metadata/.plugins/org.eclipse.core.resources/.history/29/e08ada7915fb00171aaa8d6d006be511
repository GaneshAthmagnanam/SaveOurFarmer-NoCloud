package com.yaalis.savefarmer.savefarmer.bean;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class FarmerBean {

	@Id
	private String id;
	private String name;
	private String loanAmount;
	private String monthsDue;
	private String amountPending;
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getLoanAmount() {
		return loanAmount;
	}
	public void setLoanAmount(String loanAmount) {
		this.loanAmount = loanAmount;
	}
	public String getMonthsDue() {
		return monthsDue;
	}
	public void setMonthsDue(String monthsDue) {
		this.monthsDue = monthsDue;
	}
	public String getAmountPending() {
		return amountPending;
	}
	public void setAmountPending(String amountPending) {
		this.amountPending = amountPending;
	}
	
}
